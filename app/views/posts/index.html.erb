<div class="header-text">
    <h1>Gwyneth Herbert</h1>
    <h3>Captain's Blog</h3>
  </div>
  <div class="container">  
    <div class="span9">
      <div class="container-blog">
        <% @posts.each do |post| %>
          <h2><%= post.title %></h2>
          <%= post.photo %>
            <p><%= post.created_at %></p>
            <p><%= post.content %></p>
          <% end %>
      </div>
    </div>
  </div>