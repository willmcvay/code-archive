{
  "name": "is-the-cricket-on",
  "version": "1.0.0",
  "description": "An application for cricket fans",
  "main": "server.ts",
  "repository": "https://github.com/willmcvay/isTheCricketOn",
  "scripts": {
    "deploy": "NODE_ENV=PROD make && NODE_ENV=PROD node ./server.ts",
    "test": "./node_modules/karma/bin/karma start",
    "testWatch": "./node_modules/karma/bin/karma start --auto-watch --no-single-run",
    "start": "node ./server.ts",
    "watch": "node ./scripts/compiler.js && concurrently \"node ./scripts/devServer.js\" \"nodemon ./server/server.js --watch dist -e js\"",
    "sass": "./node_modules/node-sass/bin/node-sass -i --output-style nested --include-path sass/style.sass -o public/css",
    "sassWatch": "./node_modules/nodemon/bin/nodemon.js --ext sass,scss -x sass",
    "lint": "tslint './**/*.ts{,x}'"
  },
  "author": "Will McVay",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.2",
    "express": "^4.13.3",
    "history": "^4.4.0",
    "mongodb": "^2.0.49",
    "mongoose": "^4.2.8",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.3",
    "concurrently": "^3.1.0",
    "enzyme": "~2.6.0",
    "es6-shim": "^0.35.1",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.13",
    "progress-bar-webpack-plugin": "~1.9.0",
    "promise-polyfill": "^6.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.7.4",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "webpack": "~1.13.2",
    "webpack-dev-server": "~1.16.2"
  }
}
